<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible"
              content="IE=edge">
        <meta name="viewport"
              content="width=device-width, initial-scale=1.0">
        <title>Document</title>
    </head>

    <body>
        <!--header-->
        <%- include('../partials/header') %>
        <main>
            <table>
                <thead>
                    <tr>
                        <th>Firstname</th>
                        <th>Lastname</th>
                        <th>Email</th>
                        <th>Password</th>

                    </tr>
                </thead>
                <tbody>
                    <% for (let i =0; i < users.length; i++){ %>
                    <tr>
                        <td><%= users[i].firstname %></td>
                        <td><%= users[i].lastname %></td>
                        <td><%= users[i].email %></td>
                        <td><%= users[i].password %></td>
                    </tr>


                    <% } %>
                </tbody>
            </table>
        </main>
        <!--footer-->
    </body>

</html>
